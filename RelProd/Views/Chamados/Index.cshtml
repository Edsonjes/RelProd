@model X.PagedList.IPagedList<RelProd.Models.Chamados>
@using X.PagedList
@using X.PagedList.Mvc.Core


@{
	ViewData["Title"] = "Index";
}

<h1>Demandas</h1>

<p>
	<a asp-action="Create">Criar uma nova</a> /
	<a asp-action="Relatorio">Relátorio</a>

</p>
<table class="table">
	<thead>
		<tr>
			<th>

				@Html.DisplayName("Status")

			</th>
			<th>
				@Html.DisplayName("Setor")
			</th>

			<th>
				@Html.DisplayName("Solicitante")
			</th>
			<th>
				@Html.DisplayName("DataAbertura")
			</th>
			<th>
				@Html.DisplayName("Hora")
			</th>
			<th></th>
		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model)
		{
			<tr>
				<td>
					@Html.DisplayFor(modelItem => item.Status)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.Setor)
				</td>

				<td>
					@Html.DisplayFor(modelItem => item.Solicitante)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.DataAbertura)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.Hora)
				</td>
				<td>
					<a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
					<a asp-action="Details" asp-route-id="@item.Id">Details</a> |
					<a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
				</td>
			</tr>
			
			
 }
	</tbody>
</table>
<br />
Página @Model.PageNumber de  @Model.PageCount

@Html.PagedListPager(Model, page => Url.Action("Index", new {pagina = page }))
<div>


</div>
